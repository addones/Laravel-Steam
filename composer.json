{
    "name": "addones/laravel-steam",
    "description": "A steam-api client for Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "2er7",
            "email": "ee@addones.net",
            "homepage": "https://github.com/addones/Laravel-Steam"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.5.34",
        "guzzlehttp/guzzle": "^6.0",
        "nukacode/database": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Dawoea\\SteamApi\\": "src/Dawoea/SteamApi"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dawoea\\SteamApi\\SteamApiServiceProvider"
            ],
            "aliases": {
                "Steam": "Dawoea\\SteamApi\\Facades\\SteamApi"
            }
        },
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
